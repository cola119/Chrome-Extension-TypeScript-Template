{
    "name": "Word context memory",
    "description": "extension description",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "32": "images/icon_32.png",
        "48": "images/icon_48.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ]
}